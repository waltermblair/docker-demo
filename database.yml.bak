development:
  dialect: "mysql"
  database: "coke_development"
  host: "host.docker.internal"
  port: "3306"
  user: "root"
  password: "root"
  encoding: "utf8mb4_general_ci"

test:
  url: {{envOr "TEST_DATABASE_URL" "mysql://root:root@(host.docker.internal:3306)/coke_test?parseTime=true&multiStatements=true&readTimeout=1s"}}

production:
  url: {{envOr "DATABASE_URL" "mysql://root:root@(host.docker.internal:3306)/coke_production?parseTime=true&multiStatements=true&readTimeout=1s"}}
